        "clsx": "^2.1.1",
        "framer-motion": "^11.15.0",
        "jose": "^6.1.0",
        "lucide-react": "^0.525.0",
        "next": "^15.3.5",
        "next-transpile-modules": "^10.0.1",
        "prisma": "^6.15.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
